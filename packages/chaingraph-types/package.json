{
  "name": "@badaitech/chaingraph-types",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "license": "BUSL-1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/badaitech/chaingraph.git",
    "directory": "./packages/chaingraph-types"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./edge/*": {
      "types": "./dist/edge/*.d.ts",
      "default": "./dist/edge/*.js"
    },
    "./port/*": {
      "types": "./dist/port/*.d.ts",
      "default": "./dist/port/*.js"
    },
    "./node/*": {
      "types": "./dist/node/*.d.ts",
      "default": "./dist/node/*.js"
    },
    "./flow/*": {
      "types": "./dist/flow/*.d.ts",
      "default": "./dist/flow/*.js"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "default": "./dist/utils/*.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "bun build ./src/index.ts --outdir ./dist",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.1",
    "superjson": "^2.2.2",
    "type-fest": "^4.10.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "typescript": "^5.3.3"
  }
}
