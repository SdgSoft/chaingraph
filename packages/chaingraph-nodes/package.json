  },
  "dependencies": {
    "@badaitech/chaingraph-types": "workspace:*",
    "@langchain/openai": "^0.4.2",
    "langchain": "^0.3.15",
    "lucide-react": "^0.279.0",
    "superjson": "^2.2.1"
  },
  "devDependencies": {
    "@svgr/core": "^8.0.0",
    "@svgr/plugin-jsx": "^8.0.1",
    "@svgr/plugin-svgo": "^8.0.1",
    "@types/node": "^20.11.16",
    "@types/react": "^18.3.18",
    "typescript": "^5.3.3",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^3.2.0"
  }
